
<%- include('../mainpage/headerAndNav') %>
  <!-- Body-->
  <body>
    <div class="container navTopMargin">
      <div class="row">
        <div class="col d-flex justify-content-center submitSighting">
          Create a new Podcast Series
        </div>
      </div>
<form action="/note" method="POST">
  <div class="form-group">
    <label for='podcastSeries'>Podcast Series<span class="required">*</span></label>
    <input type='text' class="form-control" id='podcastSeries' name='podcastSeries'>
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="habitat">Description</label>
      <input type="textarea" class="form-control" id="description" name='description' placeholder="What is the subject matter of the podcast?">
    </div>
  </div>
  <div class="form-group">
    <label for="appearance">Paste Artwork URL Here</label>
    <input type="textarea" class="form-control" id="podcastUrl" name='podcastUrl'>
  </div>
  
  <div class="form-group">
    <div>
      <label for="behaviour">Genre</label>
    </div>
    <div class="form-check form-check-inline">
        <input class="form-check-input" type="checkbox" id="behaviour" name="behaviour" value=""/>
        <label class="form-check-label">Test</label>
    </div>
  </div>
  
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="vocalisations">Sub Genre</label>
      <input type="text" class="form-control" id="vocalizations" name='vocalisations' placeholder ='Use mnemonics to describe birds sounds'>
    </div>
  </div>
  <div class="form-group">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
    </div>
  </body>
  
<%-include('../mainpage/footer') %>
